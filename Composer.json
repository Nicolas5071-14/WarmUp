    "type": "mautic-plugin",
    "keywords": [
        "mautic",
        "plugin",
        "email",
        "warmup",
        "deliverability"
    ],
    "extra": {
        "install-directory-name": "MauticWarmUpBundle"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "mautic/core-lib": "^6.0",
        "symfony/mailer": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MauticPlugin\\MauticWarmUpBundle\\": ""
        }
    },
